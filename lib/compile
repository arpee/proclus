rm saito/web/browser.js.gz
rm data/database.sq3
rm options
cp options.conf options
browserify browser.js -o saito/web/browser.js -i express -i sqlite3 -i express-basic-auth
cd saito/web
gzip browser.js
./compile
cd ../../
echo ""
echo ""
echo "Please update the size of the browser.js.gz file to: "
echo ""
ls -l saito/web/browser.js.gz
echo ""
echo ""


