rm -f saito/web/browser.js.gz
#rm -f data/database.sq3
#rm -f options
#cp options.conf options
browserify browser.js -o saito/web/browser.js -i express -i sqlite3 -i express-basic-auth
cd saito/web
gzip browser.js
./compile
cd ../../
echo ""
echo ""
echo "You should be ready to run Saito: "
echo ""
echo "$> node start.js "
echo ""
echo ""


